function get_location_spr(obj)
 multiplier = 1
 if frame[1] == 2 then
  multiplier = 1
 end
 location = mget(flr(((obj.pos.x*multiplier)+4)/8), flr(((obj.pos.y*multiplier)+4)/8))
 return location
end

function get_location_spr_by_coords(x, y)
 multiplier = 1
 if frame[1] == 2 then
  multiplier = 2
 end
 location = mget(flr((x+4)/8), flr((y+4)/8))
 return location
end
